<?xml version='1.0' encoding='UTF-8'?>
<interface>
  <template class="ModalImage" parent="GtkWindow">
    <property name="default-height">400</property>
    <property name="default-width">400</property>
    <property name="deletable">False</property>
    <property name="destroy-with-parent">True</property>
    <property name="modal">True</property>
    <property name="resizable">False</property>
    <property name="title">Ingresa tu contrase√±a</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="cabecera">
        <property name="show-title-buttons">False</property>
      </object>
    </child>
    <child>
      <object class="GtkBox" id="gtkbox">
        <property name="orientation">vertical</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="spacing">12</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="margin-bottom">8</property>
            <property name="margin-end">8</property>
            <property name="margin-start">8</property>
            <property name="spacing">8</property>
            <child>
              <object class="GtkImage" id="image">
                <property name="height-request">300</property>
                <property name="width-request">300</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="aceptar">
                <property name="label">Aceptar</property>
              </object>
            </child>
            <layout>
              <property name="column">0</property>
              <property name="column-span">1</property>
              <property name="row">1</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
